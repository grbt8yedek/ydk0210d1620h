{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/monitoring/cron-sample",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/backup/scheduled",
      "schedule": "0 */6 * * *"
    }
  ]
}